<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.1"
    STATUS="ALPHA"
    COMPATIBILITY="ALL"
    AUTHOR="jsminet"
    RELEASE_DATE="20131121"
    STARTABLE="true"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Orchestration</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL></URL>
  </DOCUMENTATION>

  <CONNECTORS>
  	<CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK"/>
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
  
  	<PARAMETER NAME="DIRECTORY" FIELD="DIRECTORY" NUM_ROW="1" REQUIRED="true">
      <DEFAULT>"__COMP_DEFAULT_FILE_DIR__"</DEFAULT>
    </PARAMETER><PARAMETER NAME="INCLUDE_SUBDIR" FIELD="CHECK" NUM_ROW="2">
      <DEFAULT>true</DEFAULT>
    </PARAMETER><PARAMETER NAME="MAX_ITERATIONS" FIELD="TEXT" NUM_ROW="3">
      <DEFAULT></DEFAULT>
    </PARAMETER><PARAMETER NAME="ENTRY_CREATE" FIELD="CHECK" REQUIRED="true" NUM_ROW="4">
	<DEFAULT>true</DEFAULT>
</PARAMETER><PARAMETER NAME="ENTRY_DELETE" FIELD="CHECK" REQUIRED="true" NUM_ROW="4">
	<DEFAULT>true</DEFAULT>
</PARAMETER><PARAMETER NAME="ENTRY_MODIFY" FIELD="CHECK" REQUIRED="true" NUM_ROW="4">
	<DEFAULT>true</DEFAULT>
</PARAMETER><PARAMETER NAME="THEN" FIELD="CLOSED_LIST" REQUIRED="true" NUM_ROW="5">
      <ITEMS DEFAULT="continue">
        <ITEM NAME="CONTINUE" VALUE="continue" />
        <ITEM NAME="EXIT" VALUE="exit" />
      </ITEMS>
    </PARAMETER><PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="6">
      <TABLE READONLY="true">
      	<COLUMN NAME="CURRENT_ITERATION" TYPE="id_Integer" />
      	<COLUMN NAME="ITEMNAME" TYPE="id_String" LENGTH="255" />
      	<COLUMN NAME="EVENT" TYPE="id_String" LENGTH="255" />
      	<COLUMN NAME="PREVIOUS_DIR" TYPE="id_String" LENGTH="255" />
      	<COLUMN NAME="REGISTER" TYPE="id_String" LENGTH="255" />
      	<COLUMN NAME="MIMETYPE" TYPE="id_String" LENGTH="255" /><COLUMN
      		NAME="FULLPATH" TYPE="id_String" LENGTH="255">
</COLUMN>
      	<COLUMN NAME="ISDIRECTORY" TYPE="id_Boolean" LENGTH="1"></COLUMN>
      </TABLE>
	</PARAMETER></PARAMETERS>
  
  <ADVANCED_PARAMETERS>
  </ADVANCED_PARAMETERS>

  	<CODEGENERATION>
	</CODEGENERATION>

  <RETURNS>
    <RETURN NAME="CURRENT_ITERATION" TYPE="id_Integer" AVAILABILITY="FLOW"/>
    <RETURN NAME="ITEMNAME" TYPE="id_String" AVAILABILITY="FLOW"/>
    <RETURN NAME="FILENAME" TYPE="id_String" AVAILABILITY="FLOW"/>
    <RETURN NAME="EVENT" TYPE="id_String" AVAILABILITY="FLOW"/>
    <RETURN NAME="PREVIOUS_DIR" TYPE="id_String" AVAILABILITY="FLOW"/>
    <RETURN NAME="REGISTER" TYPE="id_String" AVAILABILITY="FLOW"/>
    <RETURN NAME="MIMETYPE" TYPE="id_String" AVAILABILITY="FLOW"/><RETURN
    	NAME="FULLPATH" TYPE="id_String" AVAILABILITY="FLOW">
</RETURN>
  	<RETURN NAME="ISDIRECTORY" TYPE="id_Boolean" AVAILABILITY="FLOW"></RETURN>

  </RETURNS>
</COMPONENT>
